{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "jwt_settings": {
    "Issuer": "http://localhost:3000",
    "ExpiryTime": "8h",
    "UseRsa": true,
    "HmacSecretKey": "Secret key",
    "RsaPrivateKey": "rsa-private-key.xml",
    "RsaPublicKey": "rsa-public-key.xml"
  }
}
